  },
  "homepage": "https://github.com/paulathevalley/astro-react-uswds-example#readme",
  "dependencies": {
    "@uswds/uswds": "^3.3.0",
    "astro": "^1.9.2",
    "astro-i18next": "1.0.0-beta.17"
  },
  "devDependencies": {
    "@astrojs/react": "^1.2.2",
    "prettier-plugin-astro": "^0.7.2",
    "sass": "^1.57.1",
    "vite-plugin-svgr": "^2.4.0"
  }
