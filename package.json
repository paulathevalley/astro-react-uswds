{
  "name": "astro-react-uswds-example",
  "version": "1.0.0",
  "description": "astro with react and us web design system (uswds)",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "check:prettier": "prettier --check .",
    "check:ts": "tsc --noEmit",
    "check:astro": "astro check",
    "check": "npm run check:prettier && npm run check:ts && npm run check:astro",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "astro-i18next generate && prettier --write src/pages --loglevel warn",
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "NODE_ENV=production npm run generate",
    "build": "NODE_ENV=production astro build",
    "preview": "astro preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulathevalley/astro-react-uswds-example.git"
  },
